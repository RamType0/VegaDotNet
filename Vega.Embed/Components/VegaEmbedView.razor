@if (this.RendererInfo.IsInteractive)
{
    <InteractiveVegaEmbedView SpecJson="@SpecJson" SpecUrl="@SpecUrl" Options="@Options" />
}
else
{
    <StaticVegaEmbedView SpecJson="@SpecJson" SpecUrl="@SpecUrl" Options="@Options"/>   
}

@code {
    [Parameter]
    public string? SpecJson { get; set; }
    [Parameter]
    public Uri? SpecUrl { get; set; }
    [Parameter]
    public EmbedOptions? Options { get; set; }
}